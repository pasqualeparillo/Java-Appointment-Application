<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane accessibleRole="PASSWORD_FIELD" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginScreen">
   <children>
      <Label fx:id="loginLabel" layoutX="272.0" layoutY="14.0" text="Login">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <AnchorPane layoutX="143.0" layoutY="78.0" prefHeight="200.0" prefWidth="200.0" style="-fx-border-width: 1px; -fx-border-color: black; -fx-border-radius: 5px; -fx-padding: 5px;">
         <children>
            <Label fx:id="userPasswordLabel" layoutX="14.0" layoutY="90.0" text="Password:" />
            <Label fx:id="userNameLabel" layoutX="12.0" layoutY="26.0" text="Username:" />
            <Button fx:id="submitButton" layoutX="13.0" layoutY="166.0" mnemonicParsing="false" onAction="#submitLogin" prefHeight="26.0" prefWidth="135.0" text="Submit" />
            <Button fx:id="exitButton" layoutX="159.0" layoutY="166.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="26.0" prefWidth="135.0" text="Exit" />
            <TextField fx:id="userNameField" layoutX="13.0" layoutY="47.0" prefHeight="26.0" prefWidth="281.0" />
            <PasswordField fx:id="passwordField" layoutX="12.0" layoutY="110.0" prefHeight="26.0" prefWidth="282.0" />
         </children>
      </AnchorPane>
      <Label fx:id="userLocal" layoutX="14.0" layoutY="369.0" text="User Lang:" />
   </children>
</AnchorPane>
